{
	"rewrites": [
		{
			"source": "/v2gen/(.*)",
			"destination": "/v2gen/"
		}
	],
	"redirects": [
		{
			"source": "/donate",
			"destination": "/about#打赏",
			"permanent": false
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=63072000; includeSubDomains; preload"
				}
			]
		}
	]
}
